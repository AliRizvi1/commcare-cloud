#
# {{ ansible_managed }}
#
init_config:
  custom_jar_paths: # optional
    - {{ code_home }}/formplayer_build/formplayer.jar

instances:
  - host: localhost
    port: 9999

    name: jmx_formplayer_instance

    tags:
      env: {{ deploy_env }}
      newTag: formplayer

    conf:
      - include:
          bean_regex:
            - .*Controller.*
            - .*Service.*
            - .*Factory.*
            - .*Aspect.*
            - .*Filter.*
